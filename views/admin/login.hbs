                <div class="text-center m-b-md custom-login">
                    <h3>Vui Lòng Đăng Nhập</h3>
                </div>
                <div class="hpanel">
                    <div class="panel-body">
                        <form action="/login" id="loginForm" method="POST">
                            <div class="form-group">
                                <label class="control-label" for="username">Tên Đăng Nhập</label>
                                <input type="text" placeholder="" title="Please enter you username" required="" value="" name="username" id="username" class="form-control">
                                <span class="help-block small"></span>
                            </div>
                            <div class="form-group">
                                <label class="control-label" for="password">Mật Khẩu</label>
                                <input type="password" title="Please enter your password" placeholder="" required="" value="" name="password" id="password" class="form-control">
                                <span class="help-block small"></span>
                            </div>
                           
                            <input class="btn btn-success btn-block loginbtn" type="submit" value="Đăng Nhập">
                            <a class="btn btn-default btn-block" href="#">Đăng Kí</a>
                        </form>
                    </div>
                </div>
            </div>
