<!-- Single pro tab start-->
<div class="single-product-tab-area mg-tb-15">
    <!-- Single pro tab review Start-->
    <div class="single-pro-review-area">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="review-tab-pro-inner">
                        <ul id="myTab3" class="tab-review-design">
                            <li class="active"><a href="#description"><i class="fa fa-pencil" aria-hidden="true"></i> Manufacturer Edit</a></li>
                            <li><a href="#reviews"><i class="fa fa-file-image-o" aria-hidden="true"></i> Pictures</a></li>
                            <li><a href="#INFORMATION"><i class="fa fa-commenting" aria-hidden="true"></i> Review</a></li>
                        </ul>
                        <div id="myTabContent" class="tab-content custom-product-edit">
                            <div class="product-tab-list tab-pane fade active in" id="description">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <form action="/commit{{object_id}}" method="post">
                                        <div class="review-content-section">
                                            <div class="input-group mg-b-pro-edt">
                                                <span class="input-group-addon"><i class="fa fa-user" aria-hidden="true"></i></span>
                                                <input type="text" class="form-control" placeholder="Tên" id="name" name="name">
                                            </div>
                                            <div class="input-group mg-b-pro-edt">
                                                <span class="input-group-addon"><i class="fa fa-pencil" aria-hidden="true"></i></span>
                                                <input type="text" class="form-control" placeholder="Địa chỉ" id="address" name="address">
                                            </div>
                                            <div class="input-group mg-b-pro-edt">
                                                <span class="input-group-addon"><i class="fa fa-sticky-note-o" aria-hidden="true"></i></span>
                                                <input type="text" class="form-control" placeholder="Số điện thoại" id="phone" name="phone">
                                            </div>
                                            <div class="input-group mg-b-pro-edt">
                                                <span class="input-group-addon"><i class="fa fa-qrcode" aria-hidden="true"></i></span>
                                                <input type="text" class="form-control" placeholder="Mô tả" id="des" name="des">
                                            </div>
                                        </div>
                                      </div>
                                      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="product-status-wrap">
                                            <h4>Product List</h4>
                                            <table>
                                                <tr>
                                                    <th>Tên sản phầm</th>
                                                    <th>Màu</th>
                                                    <th>Cỡ</th>
                                                    <th>Số lượng</th>
                                                    <th>Giá</th>
                                                    <th>Xoá</th>
                                                </tr>
                                                {{#each listOrder}}
                                                <tr>
                                                    <td>{{this.ReceiverName}}</td>
                                                    <td>{{this.Address}}</td>
                                                    <td>{{this.ReceiverPhonenumber}}</td>
                                                    <td>{{this.OrderDate}}</td>
                                                    <td>
                                                      <form action="/order-trash-{{this._id}}" method="POST"><button data-toggle="tooltip" title="Trash" class="pd-setting-ed" type="submit"><i class="fa fa-trash-o" aria-hidden="true"></i></button></form>
                                                    </td>
                                                </tr>
                                                {{/each}}
                                            </table>
                                            <div class="custom-pagination">
                                                <nav aria-label="Page navigation example">
                                                    <ul class="pagination">
                                                        <li class="page-item"><a class="page-link" href="#">Previous</a></li>
                                                        <li class="page-item"><a class="page-link" href="#">1</a></li>
                                                        <li class="page-item"><a class="page-link" href="#">2</a></li>
                                                        <li class="page-item"><a class="page-link" href="#">3</a></li>
                                                        <li class="page-item"><a class="page-link" href="#">Next</a></li>
                                                    </ul>
                                                </nav>
                                            </div>
                                        </div>
                                      </div>
                                  </div>
                                  <div class="row">
                                      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                          <div class="text-center mg-b-pro-edt custom-pro-edt-ds">
                                              <button type="submit" class="btn btn-primary waves-effect waves-light m-r-10">Save
                                                  </button>
                                              <form action="manufacturer-list" method="get"><button type="submit" class="btn btn-warning waves-effect waves-light">Discard
                                                  </button>
                                                </form>
                                          </div>
                                      </div>
                                  </div>
                            </div>
